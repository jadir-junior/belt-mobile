{
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "expo:start": "expo start",
    "expo:android": "expo start --android",
    "expo:ios": "expo start --ios",
    "expo:web": "expo start --web",
    "expo:eject": "expo eject",
    "test:dev": "jest --watch --coverage=false",
    "test:debug": "jest -o --watch --coverage=false",
    "test:update:snapshoots": "jest -u --coverage=false",
    "test": "jest",
    "test:e2e": "detox test -c android.emu.debug",
    "style:check": "prettier --check '**/*.tsx'",
    "style:fix": "prettier --write '**/*.tsx'",
    "generate:component": "yarn plop --plopfile generators/component.js",
    "generate:screen": "yarn plop --plopfile generators/screen.js",
    "prepare": "husky install",
    "ios": "react-native run-ios"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|sentry-expo|native-base)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^2.8.2",
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-navigation/drawer": "^6.1.6",
    "@react-navigation/native": "^6.0.4",
    "@react-navigation/native-stack": "^6.2.2",
    "@testing-library/react-native": "^7.2.0",
    "@types/styled-components-react-native": "^5.1.1",
    "axios": "^0.22.0",
    "expo": "~42.0.1",
    "expo-font": "~9.2.1",
    "expo-splash-screen": "~0.11.2",
    "expo-status-bar": "~1.0.4",
    "expo-updates": "~0.8.1",
    "jwt-decode": "^3.1.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "^7.17.3",
    "react-native": "~0.63.4",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-reanimated": "~2.2.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.4.0",
    "react-native-unimodules": "0.14.10",
    "react-native-web": "~0.13.12",
    "styled-components": "^5.3.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/user-event": "^13.3.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "^27.0.2",
    "@types/jwt-decode": "^3.1.0",
    "@types/prettier": "^2.4.1",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "@types/styled-components": "^5.1.15",
    "detox": "^18.22.1",
    "detox-expo-helpers": "^0.6.0",
    "eslint-plugin-testing-library": "^4.12.4",
    "expo-detox-hook": "^1.0.10",
    "husky": "^7.0.2",
    "jest-circus": "^27.2.4",
    "jest-expo": "^42.1.0",
    "msw": "^0.35.0",
    "plop": "^2.7.4",
    "prettier": "^2.4.1",
    "react-native-url-polyfill": "^1.3.0",
    "reactotron-react-native": "^5.0.0",
    "typescript": "~4.0.0"
  },
  "private": true,
  "name": "belt-mobile",
  "version": "1.0.0"
}
